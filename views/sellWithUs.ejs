  <script>
    function submitSellForm() {

      let name = $('#floatingInputName1').val()
      let phone = $('#floatingInputPhone1').val()
      let email = $('#floatingInputEmail1').val()
      let pincode = $('#floatingInputPincode').val()
      let formtype = $('#mobi-select').val()

      if (!name || !phone || !email || !pincode || !formtype) {
        iziToast.warning({
          title: 'warning',
          message: 'Please Fill All the Field !',
        });
        return;
      }

      if (formtype == 'Select Type') {
        iziToast.warning({
          title: 'warning',
          message: 'Please Select Type !',
        });
        return;
      }



      $.ajax({
        url: '/sellWithUs',
        method: "POST",
        data: {
          name: name,
          phone: phone,
          email: email,
          pincode: pincode,
          formtype: formtype,
        },
        success: (res) => {
          iziToast.success({
            title: 'OK',
            message: 'Successfully Submitted !',
          });
          $('#floatingInputName1').val('')
          $('#floatingInputPhone1').val('')
          $('#floatingInputEmail1').val('')
          $('#floatingInputPincode').val('')
          $('#mobi-select').val('Select Type')
          // $("#sellForm").load(location.href + " #sellForm");
        }
      })
    } 
  </script>

